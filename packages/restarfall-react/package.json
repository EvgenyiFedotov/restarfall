{
  "name": "restarfall-react",
  "version": "0.1.7",
  "license": "MIT",
  "author": "Evgenii Fedotov",
  "description": "Restarfall is a JavaScript library for creating business logic and data management.",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "require": "./dist/mjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -fr ./dist && webpack -c ./webpack/prod.js ",
    "start": "rm -fr ./dist && webpack -c ./webpack/dev.js --watch"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "restarfall": "file:../restarfall"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.21",
    "jest-environment-jsdom": "^29.6.4"
  }
